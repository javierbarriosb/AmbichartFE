{
  "arrowParens": "always",
	"bracketSpacing": true,
  "bracketSameLine": false,
	"insertPragma": false,
	"printWidth": 1000,
	"proseWrap": "preserve",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "none",
	"useTabs": false,
	"endOfLine": "auto"
}
